{
  "name": "nextjs-reference-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.17.1",
    "npm": "9.6.7",
    "yarn": "yarnではなくnpmを使用してください"
  },
  "scripts": {
    "dev": "next dev",
